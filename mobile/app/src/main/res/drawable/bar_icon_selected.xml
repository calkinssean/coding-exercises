<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:width="36dp"
                android:height="36dp" />
            <solid
                android:color="#673AB7" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <size
                android:width="36dp"
                android:height="36dp" />
            <stroke
                android:width="3dp"
                android:color="#FFFFFF" />
        </shape>
    </item>
    <item
        android:id="@+id/inner_icon"
        android:width="20dp"
        android:height="20dp"
        android:drawable="@drawable/ic_bar"
        android:gravity="center" />
</layer-list>